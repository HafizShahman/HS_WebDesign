<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>EchoValidationData</servlet-name>
        <servlet-class>EchoValidationData</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletInfo</servlet-name>
        <servlet-class>ServletInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RadioBtnServlet</servlet-name>
        <servlet-class>RadioBtnServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EchoValidationData</servlet-name>
        <url-pattern>/EchoValidationData</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletInfo</servlet-name>
        <url-pattern>/ServletInfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RadioBtnServlet</servlet-name>
        <url-pattern>/RadioBtnServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>StudentValidation.html</welcome-file>
        <welcome-file>WebDirectory.html</welcome-file>
    </welcome-file-list>
</web-app>
